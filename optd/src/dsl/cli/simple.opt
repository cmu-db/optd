data Logical =
    | Add(left: Logical, right: Logical)
    \ Const(val: I64)

data Physical
data LogicalProperties
data PhysicalProperties

fn (log: Logical) fold: I64 = match log
    | Add(left, right) -> fold(left)() + fold(right)
    | _ -> if 5 > 3 then 5 else 0
    | _ -> {
        let v: Logical = Const(5) in
        v
    }
    \ Const(val) -> val