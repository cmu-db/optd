// Basic generic function for array mapping
fn <T, U> (input: [T]) map(transform: T -> U): [U] = match input
| [] -> []
\ [x .. xs] -> [transform(x)] ++ xs.map(transform)

fn main(): [I64] = 
let input = [1, 2, 3] in
input.map(x: I64 -> x + 1)  // Should return [2, 3, 4]